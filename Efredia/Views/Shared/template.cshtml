    ViewBag.Twitter = "@aslunFR";
    ViewBag.NameDev = "AslunFR";
    Response.AppendHeader("Access-Control-Allow-Origin", "*");
    ViewBag.Project = "Efredia Framewok";
    ViewBag.urlcontent = "http://localhost";

}

